{
    "name": "awladdeleo/faker-news",
    "description": "Faker provider for fake news!",
    "keywords": [
        "awladdeleo",
        "faker-news",
        "faker",
        "data"
    ],
    "homepage": "https://github.com/awladdeleo/faker-news",
    "license": "MIT",
    "type": "library",
    "authors": [{
        "name": "Paul Hennell",
        "email": "git@hennell.dev",
        "role": "Developer"
    }],
    "require": {
        "php": "^7.3|^8.0",
        "fzaninotto/faker": "^1.9.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "Faker\\Provider\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "awladdeleo\\FakerNews\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
